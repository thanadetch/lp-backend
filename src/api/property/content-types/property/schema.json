{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "bed": {
      "type": "integer"
    },
    "bath": {
      "type": "integer"
    },
    "sqm": {
      "type": "decimal"
    },
    "rentalPrice": {
      "type": "integer"
    },
    "salePrice": {
      "type": "integer"
    },
    "ownerName": {
      "type": "string"
    },
    "ownerPhoneNo": {
      "type": "string"
    },
    "subCode": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-code.sub-code",
      "inversedBy": "properties"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "unitNo": {
      "type": "string"
    },
    "floor": {
      "type": "string"
    },
    "isPetFriendly": {
      "type": "boolean",
      "default": false
    },
    "propertyType": {
      "type": "enumeration",
      "enum": [
        "condo",
        "apartment",
        "service_apartment",
        "house",
        "townhouse",
        "penthouse",
        "duplex",
        "commercial",
        "land"
      ]
    },
    "ownerEmail": {
      "type": "string"
    },
    "ownerLineId": {
      "type": "string"
    },
    "listingType": {
      "type": "enumeration",
      "enum": [
        "rent",
        "sell",
        "rent_and_sell"
      ],
      "required": true
    },
    "postBy": {
      "type": "enumeration",
      "enum": [
        "landlord",
        "agent"
      ]
    },
    "propertyCode": {
      "type": "uid",
      "required": true
    }
  }
}
